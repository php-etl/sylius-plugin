{
    "name": "php-etl/sylius-plugin",
    "description": "Adapters for the Sylius API client",
    "type": "gyroscops-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2",
        "php-etl/configurator-contracts": "^0.5.0",
        "php-etl/packaging-contracts": "^0.1.0",
        "php-etl/fast-map-plugin": "^0.7.0",
        "php-etl/satellite-toolbox": "^0.3.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.4@dev",
        "symfony/yaml": "^5.2",
        "php-etl/bucket-contracts": "^0.1.0",
        "phpunit/phpunit": "^9.0",
        "laminas/laminas-diactoros": "^2.6",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*",
        "php-etl/sylius-api-php-client": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.10",
        "php-etl/phpunit-extension": "0.4.x-dev",
        "php-etl/bucket": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\Sylius\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\Sylius\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.6.x-dev"
        },
        "gyroscops": {
            "plugins": ["Kiboko\\Plugin\\Sylius\\Service"]
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
